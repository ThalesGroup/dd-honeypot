{"path": "", "args": {}, "content": "<!DOCTYPE html>\n<html lang=\"en\" dir=\"ltr\">\n<head>\n    <meta charset=\"utf-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\n    <title>phpMyAdmin</title>\n</head>\n<body id=\"loginform\">\n    <div class=\"container\">\n        <a href=\"./index.php\" class=\"logo\">\n            <h1>phpMyAdmin</h1>\n        </a>\n        <form method=\"post\" id=\"login_form\" action=\"index.php\" name=\"login_form\" class=\"disableAjax login hide js-show\">\n            <fieldset>\n                <legend>Log in</legend>\n                <input type=\"hidden\" name=\"token\" value=\"\">\n                <input type=\"hidden\" name=\"set_session\" value=\"1\">\n                <div class=\"item\">\n                    <label for=\"input_username\">Username:</label>\n                    <input type=\"text\" name=\"pma_username\" id=\"input_username\" value=\"\" size=\"24\" class=\"textfield\">\n                </div>\n                <div class=\"item\">\n                    <label for=\"input_password\">Password:</label>\n                    <input type=\"password\" name=\"pma_password\" id=\"input_password\" value=\"\" size=\"24\" class=\"textfield\">\n                </div>\n                <div class=\"item\">\n                    <input type=\"checkbox\" name=\"pma_remember\" id=\"input_remember\" value=\"yes\">\n                    <label for=\"input_remember\">Remember me</label>\n                </div>\n                <div class=\"item\">\n                    <input id=\"input_go\" name=\"input_go\" type=\"submit\" value=\"Go\" class=\"btn\">\n                </div>\n            </fieldset>\n        </form>\n    </div>\n</body>\n</html>"}
{"path": "index.php", "args": {}, "content": "<html>\n<head>\n    <title>phpMyAdmin</title>\n</head>\n<body>\n    <div id=\"pma_navigation\" data-config-navigation-width=\"240\">\n        <div id=\"pma_navigation_resizer\"></div>\n        <div id=\"pma_navigation_collapser\"></div>\n        <div id=\"pma_navigation_content\">\n            <div id=\"pma_navigation_header\">\n                <a class=\"hide navigation_url\" href=\"index.php?route=/\"></a>\n            </div>\n            <div id=\"pma_navigation_tree\" class=\"list_container synced highlight\">\n                <div class=\"pma_quick_warp\">\n                    <div class=\"drop_list\">\n                        <button class=\"drop_button btn\">Recent</button>\n                        <ul id=\"pma_recent_list\">\n                            <li class=\"warp_link\">\n                                There are no recent tables.\n                            </li>\n                        </ul>\n                    </div>\n                    <div class=\"drop_list\">\n                        <button class=\"drop_button btn\">Favorite</button>\n                        <ul id=\"pma_favorite_list\">\n                            <li class=\"warp_link\">\n                                There are no favorite tables.\n                            </li>\n                        </ul>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div id=\"pma_sql_textarea_auto_height\">\n        <div id=\"pma_console_container\">\n            <div id=\"pma_console\">\n                <div class=\"toolbar collapsed\">\n                    <div class=\"switch_button console_switch\">Console</div>\n                    <div class=\"button clear\">\n                        <span>Clear</span>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div id=\"page_content\">\n        <div id=\"maincontainer\">\n            <div id=\"main_pane_left\">\n                <div class=\"group\">\n                    <h2>General settings</h2>\n                    <ul>\n                        <li><a class=\"item\" href=\"index.php?route=/preferences/manage\">Features</a></li>\n                        <li><a class=\"item\" href=\"index.php?route=/preferences/manage\">Main panel</a></li>\n                        <li><a class=\"item\" href=\"index.php?route=/preferences/manage\">Navigation panel</a></li>\n                    </ul>\n                </div>\n            </div>\n        </div>\n    </div>\n</body>\n</html>"}
{"path": "index.php", "args": {"route": "/server/databases"}, "content": "<div id=\"page_content\">\n    <h2>Databases</h2>\n    <ul>\n        <li class=\"database\">\n            <a href=\"index.php?route=/database/structure&db=information_schema\">information_schema</a>\n        </li>\n        <li class=\"database\">\n            <a href=\"index.php?route=/database/structure&db=mysql\">mysql</a>\n        </li>\n        <li class=\"database\">\n            <a href=\"index.php?route=/database/structure&db=performance_schema\">performance_schema</a>\n        </li>\n        <li class=\"database\">\n            <a href=\"index.php?route=/database/structure&db=phpmyadmin\">phpmyadmin</a>\n        </li>\n        <li class=\"database\">\n            <a href=\"index.php?route=/database/structure&db=test\">test</a>\n        </li>\n    </ul>\n    <form id=\"create_database_form\" method=\"post\" action=\"index.php?route=/server/databases\" class=\"ajax\">\n        <fieldset>\n            <legend>Create database</legend>\n            <input type=\"hidden\" name=\"token\" value=\"token_value\">\n            <input type=\"text\" name=\"new_db\" maxlength=\"64\" class=\"textfield\" id=\"text_create_db\" required>\n            <input type=\"submit\" value=\"Create\" id=\"buttonGo\">\n        </fieldset>\n    </form>\n</div>"}
{"path": "index.php", "args": {"route": "/server/sql"}, "content": "<div id=\"page_content\">\n    <h2>SQL Query</h2>\n    <form method=\"post\" action=\"index.php?route=/server/sql\" class=\"ajax lock-page\">\n        <div class=\"card mb-3\">\n            <div class=\"card-header\">Run SQL query/queries on server</div>\n            <div class=\"card-body\">\n                <textarea name=\"sql_query\" id=\"sqlquery\" cols=\"40\" rows=\"15\" class=\"form-control\"></textarea>\n            </div>\n            <div class=\"card-footer\">\n                <input type=\"hidden\" name=\"server\" value=\"1\">\n                <input type=\"hidden\" name=\"db\" value=\"\">\n                <input type=\"hidden\" name=\"table\" value=\"\">\n                <input type=\"hidden\" name=\"token\" value=\"token_value\">\n                <input class=\"btn btn-primary\" type=\"submit\" id=\"button_submit_query\" name=\"SQL\" value=\"Go\">\n            </div>\n        </div>\n    </form>\n    <div id=\"sqlqueryresults\"></div>\n</div>\n\n<div id=\"selflink\" class=\"print_ignore\">\n    <a href=\"index.php?route=/server/sql\" title=\"Open new phpMyAdmin window\" target=\"_blank\" rel=\"noopener noreferrer\">\n        Open new phpMyAdmin window\n    </a>\n</div>"}
{"path": "index.php", "args": {"route": "/server/status"}, "content": "<div id=\"page_content\">\n    <h2>Server status</h2>\n    <div id=\"serverStatusTabs\">\n        <ul>\n            <li><a href=\"#statustabs_traffic\">Network traffic</a></li>\n            <li><a href=\"#statustabs_queries\">Query statistics</a></li>\n            <li><a href=\"#statustabs_allvars\">All status variables</a></li>\n        </ul>\n        <div id=\"statustabs_traffic\" class=\"clearfloat\">\n            <table id=\"serverstatustraffic\" class=\"data noclick\">\n                <thead>\n                    <tr>\n                        <th>Traffic</th>\n                        <th>Per hour</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    <tr>\n                        <th class=\"name\">Received</th>\n                        <td class=\"value\">67.0 MiB</td>\n                    </tr>\n                    <tr>\n                        <th class=\"name\">Sent</th>\n                        <td class=\"value\">230.0 MiB</td>\n                    </tr>\n                    <tr>\n                        <th class=\"name\">Total</th>\n                        <td class=\"value\">297.0 MiB</td>\n                    </tr>\n                </tbody>\n            </table>\n        </div>\n        <div id=\"statustabs_queries\" class=\"clearfloat\">\n            <table id=\"serverstatusqueries\" class=\"data noclick\">\n                <thead>\n                    <tr>\n                        <th>Statements</th>\n                        <th>Per hour</th>\n                        <th>%</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    <tr>\n                        <th class=\"name\">Statements</th>\n                        <td class=\"value\">44,890</td>\n                        <td class=\"value\">100%</td>\n                    </tr>\n                    <tr>\n                        <th class=\"name\">Queries</th>\n                        <td class=\"value\">44,890</td>\n                        <td class=\"value\">100%</td>\n                    </tr>\n                </tbody>\n            </table>\n        </div>\n        <div id=\"statustabs_allvars\" class=\"clearfloat\">\n            <table id=\"serverstatusvarables\" class=\"data noclick\">\n                <thead>\n                    <tr>\n                        <th>Variable</th>\n                        <th>Value</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    <tr>\n                        <th class=\"name\">Aborted_clients</th>\n                        <td class=\"value\">0</td>\n                    </tr>\n                    <tr>\n                        <th class=\"name\">Aborted_connects</th>\n                        <td class=\"value\">0</td>\n                    </tr>\n                    <!-- More variables would be listed here -->\n                </tbody>\n            </table>\n        </div>\n    </div>\n</div>\n\n<div id=\"selflink\" class=\"print_ignore\">\n    <a href=\"index.php?route=/server/status\" title=\"Open new phpMyAdmin window\" target=\"_blank\" rel=\"noopener noreferrer\">\n        Open new phpMyAdmin window\n    </a>\n</div>"}
{"path": "index.php", "args": {"route": "/server/export"}, "content": "<div id=\"page_content\">\n<h2>Exporting databases from the current server</h2>\n\n<form method=\"post\" action=\"index.php?route=/server/export\" name=\"dump\">\n    <input type=\"hidden\" name=\"token\" value=\"123456789\">\n\n    <div class=\"exportoptions\" id=\"databases_and_tables\">\n        <h3>Databases:</h3>\n        <div>\n            <select name=\"db_select[]\" id=\"db_select\" size=\"10\" multiple>\n                <option value=\"information_schema\">information_schema</option>\n                <option value=\"mysql\">mysql</option>\n                <option value=\"performance_schema\">performance_schema</option>\n                <option value=\"phpmyadmin\">phpmyadmin</option>\n                <option value=\"test\">test</option>\n            </select>\n        </div>\n\n        <div>\n            <a href=\"#\" id=\"select_all_databases\">Select all</a> /\n            <a href=\"#\" id=\"unselect_all_databases\">Unselect all</a>\n        </div>\n\n        <div>\n            <input type=\"checkbox\" name=\"structure_or_data_forced\" value=\"structure\" id=\"checkbox_dump_structure\">\n            <label for=\"checkbox_dump_structure\">Structure</label>\n        </div>\n        <div>\n            <input type=\"checkbox\" name=\"structure_or_data_forced\" value=\"data\" id=\"checkbox_dump_data\">\n            <label for=\"checkbox_dump_data\">Data</label>\n        </div>\n    </div>\n\n    <div class=\"exportoptions\" id=\"output\">\n        <h3>Output:</h3>\n        <div>\n            <input type=\"radio\" name=\"output_format\" value=\"sendit\" id=\"radio_dump_asfile\" checked>\n            <label for=\"radio_dump_asfile\">Save output to a file</label>\n        </div>\n        <div>\n            <input type=\"radio\" name=\"output_format\" value=\"none\" id=\"radio_dump_astext\">\n            <label for=\"radio_dump_astext\">View output as text</label>\n        </div>\n    </div>\n\n    <div class=\"exportoptions\" id=\"format\">\n        <h3>Format:</h3>\n        <div>\n            <select name=\"format\" id=\"format\">\n                <option value=\"sql\" selected=\"selected\">SQL</option>\n                <option value=\"csv\">CSV</option>\n                <option value=\"xml\">XML</option>\n                <option value=\"yaml\">YAML</option>\n            </select>\n        </div>\n    </div>\n\n    <div class=\"exportoptions\" id=\"compression\">\n        <h3>Compression:</h3>\n        <div>\n            <input type=\"radio\" name=\"compression\" value=\"none\" id=\"radio_compression_none\" checked>\n            <label for=\"radio_compression_none\">None</label>\n        </div>\n        <div>\n            <input type=\"radio\" name=\"compression\" value=\"zip\" id=\"radio_compression_zip\">\n            <label for=\"radio_compression_zip\">zipped</label>\n        </div>\n        <div>\n            <input type=\"radio\" name=\"compression\" value=\"gzip\" id=\"radio_compression_gzip\">\n            <label for=\"radio_compression_gzip\">gzipped</label>\n        </div>\n    </div>\n\n    <div class=\"exportoptions\" id=\"charset\">\n        <h3>Character set of the file:</h3>\n        <select name=\"charset\" id=\"select_charset\">\n            <option value=\"utf-8\" selected=\"selected\">utf-8</option>\n            <option value=\"iso-8859-1\">iso-8859-1</option>\n        </select>\n    </div>\n\n    <div class=\"exportoptions\" id=\"submit\">\n        <input type=\"submit\" value=\"Go\" id=\"buttonGo\">\n    </div>\n</form>\n</div>"}
{"path": "index.php", "args": {"route": "/server/import"}, "content": "<div id=\"page_content\">\n<h2>Import</h2>\n\n<ul>\n  <li>\n    <form action=\"index.php?route=/server/import\" method=\"post\" enctype=\"multipart/form-data\" name=\"import\" id=\"import\">\n      <input type=\"hidden\" name=\"MAX_FILE_SIZE\" value=\"2097152\">\n      <div class=\"importoptions\">\n        <h3>File to import:</h3>\n        <div class=\"formelementrow\" id=\"upload_form_status\" style=\"display: none;\"></div>\n        <div class=\"formelementrow\" id=\"upload_form\">\n          <label for=\"input_import_file\">Browse your computer:</label>\n          <input type=\"file\" name=\"import_file\" id=\"input_import_file\">\n          <br><br>\n          <input type=\"checkbox\" name=\"local_import_file\" id=\"checkbox_local_import_file\">\n          <label for=\"checkbox_local_import_file\">Select from the web server upload directory <b></b></label>\n        </div>\n      </div>\n\n      <div class=\"importoptions\">\n        <h3>Partial import:</h3>\n        <div class=\"formelementrow\">\n          <input type=\"checkbox\" name=\"allow_interrupt\" value=\"yes\" id=\"checkbox_allow_interrupt\" checked=\"checked\">\n          <label for=\"checkbox_allow_interrupt\">Allow the interruption of an import in case the script detects it is close to the PHP timeout limit.</label><br>\n        </div>\n      </div>\n\n      <div class=\"importoptions\">\n        <h3>Format:</h3>\n        <div class=\"formelementrow\">\n          <input type=\"radio\" name=\"format\" value=\"sql\" id=\"radio_sql\" checked=\"checked\">\n          <label for=\"radio_sql\">SQL</label>\n          <input type=\"radio\" name=\"format\" value=\"csv\" id=\"radio_csv\">\n          <label for=\"radio_csv\">CSV</label>\n          <input type=\"radio\" name=\"format\" value=\"xml\" id=\"radio_xml\">\n          <label for=\"radio_xml\">XML</label>\n        </div>\n      </div>\n\n      <div class=\"importoptions\" id=\"format_specific_opts\">\n        <h3>Format-specific options:</h3>\n        <div class=\"formelementrow\" id=\"sql_options\">\n          <input type=\"checkbox\" name=\"sql_compatibility\" id=\"checkbox_sql_compatibility\" value=\"NONE\">\n          <label for=\"checkbox_sql_compatibility\">Enable foreign key checks</label>\n        </div>\n      </div>\n\n      <div class=\"importoptions\">\n        <h3>Other options:</h3>\n        <div class=\"formelementrow\">\n          <input type=\"checkbox\" name=\"skip_queries\" value=\"1\" id=\"checkbox_skip_queries\" checked=\"checked\">\n          <label for=\"checkbox_skip_queries\">Skip queries</label> <input type=\"text\" name=\"skip_queries_count\" value=\"0\" size=\"6\">\n          <div class=\"formelementrow\">\n            <input type=\"checkbox\" name=\"quick_export\" value=\"1\" id=\"checkbox_quick_export\">\n            <label for=\"checkbox_quick_export\">Skip table structure</label>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"importoptions\">\n        <h3>Execution options:</h3>\n        <div class=\"formelementrow\">\n          <input type=\"checkbox\" name=\"asfile\" value=\"sendit\" id=\"checkbox_dump_asfile\" checked=\"checked\">\n          <label for=\"checkbox_dump_asfile\">Save as file</label>\n          <select name=\"compression\" id=\"select_compression\">\n            <option value=\"none\">None</option>\n            <option value=\"zip\">zipped</option>\n            <option value=\"gzip\">gzipped</option>\n          </select>\n        </div>\n      </div>\n\n      <div class=\"importoptions\">\n        <input type=\"submit\" value=\"Go\" id=\"buttonGo\">\n      </div>\n    </form>\n  </li>\n</ul>\n</div>"}
{"path": "index.php", "args": {"route": "/server/variables"}, "content": "<div id=\"page_content\">\n    <h2>Server variables and settings</h2>\n    <fieldset class=\"optbox\">\n        <legend>Filters</legend>\n        <div>\n            <div class=\"formelement\">\n                <label for=\"filterText\">Containing the word:</label>\n                <input name=\"filterText\" type=\"text\" id=\"filterText\" style=\"width: 10em\">\n            </div>\n        </div>\n        <div class=\"clearfloat\"></div>\n    </fieldset>\n    <div id=\"linkSuggestions\" class=\"jsonly hide\">\n        <div class=\"filterHeader\">\n            Suggestions\n        </div>\n        <div class=\"suggestionList\"></div>\n    </div>\n    <div id=\"serverVariables\" class=\"data filterable\">\n        <table id=\"serverVariablesTable\" class=\"data\">\n            <thead>\n                <tr>\n                    <th>Variable</th>\n                    <th>Value</th>\n                </tr>\n            </thead>\n            <tbody>\n                <tr class=\"odd\">\n                    <td class=\"name\">auto_increment_increment</td>\n                    <td class=\"value\"><span class=\"attention\">1</span></td>\n                </tr>\n                <tr class=\"even\">\n                    <td class=\"name\">auto_increment_offset</td>\n                    <td class=\"value\"><span class=\"attention\">1</span></td>\n                </tr>\n                <!-- More rows would be here -->\n            </tbody>\n        </table>\n    </div>\n</div>\n\n<form action=\"index.php?route=/server/variables\" method=\"post\" class=\"config-form disableAjax\">\n    <input type=\"hidden\" name=\"token\" value=\"token_value\">\n    <input type=\"hidden\" name=\"submit_save\" value=\"1\">\n    <fieldset class=\"optbox\">\n        <legend>Variables</legend>\n        <div class=\"formelement\">\n            <label for=\"vars_long_query_time\">\n                <code>long_query_time</code>\n            </label>\n            <input type=\"text\" size=\"40\" name=\"vars[long_query_time]\" id=\"vars_long_query_time\" value=\"10.000000\">\n            <a class=\"restore-default hide\" href=\"#vars_long_query_time\" title=\"Restore default value\">\n                <img src=\"themes/dot.gif\" title=\"\" alt=\"\" class=\"icon ic_s_reload\">\n            </a>\n        </div>\n        <!-- More variable inputs would be here -->\n    </fieldset>\n    <fieldset class=\"tblFooters\">\n        <input type=\"submit\" name=\"submit\" value=\"Apply\">\n    </fieldset>\n</form>"}
