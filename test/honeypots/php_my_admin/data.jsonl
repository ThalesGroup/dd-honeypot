{"path": "", "args": {}, "content": "<!DOCTYPE html>\n<html lang=\"en\" dir=\"ltr\">\n<head>\n    <meta charset=\"utf-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\n    <title>phpMyAdmin</title>\n</head>\n<body id=\"loginform\">\n    <div class=\"container\">\n        <a href=\"./index.php\" class=\"logo\">\n            <h1>phpMyAdmin</h1>\n        </a>\n        <form method=\"post\" id=\"login_form\" action=\"index.php\" name=\"login_form\" class=\"disableAjax login hide js-show\">\n            <fieldset>\n                <legend>Log in</legend>\n                <input type=\"hidden\" name=\"token\" value=\"\">\n                <input type=\"hidden\" name=\"set_session\" value=\"1\">\n                <div class=\"item\">\n                    <label for=\"input_username\">Username:</label>\n                    <input type=\"text\" name=\"pma_username\" id=\"input_username\" value=\"\" size=\"24\" class=\"textfield\">\n                </div>\n                <div class=\"item\">\n                    <label for=\"input_password\">Password:</label>\n                    <input type=\"password\" name=\"pma_password\" id=\"input_password\" value=\"\" size=\"24\" class=\"textfield\">\n                </div>\n                <div class=\"item\">\n                    <input type=\"checkbox\" name=\"pma_remember\" id=\"input_remember\" value=\"yes\">\n                    <label for=\"input_remember\">Remember me</label>\n                </div>\n                <div class=\"item\">\n                    <input id=\"input_go\" name=\"input_go\" type=\"submit\" value=\"Go\" class=\"btn\">\n                </div>\n            </fieldset>\n        </form>\n    </div>\n</body>\n</html>"}
{"path": "index.php", "args": {}, "content": "<div id=\"page_content\">\n    <div id=\"pma_navigation_resizer\"></div>\n    <div id=\"pma_navigation\" data-config-navigation-width=\"240\">\n        <div id=\"pma_navigation_header\">\n            <div id=\"pmalogo\">\n                <a href=\"index.php\">\n                    <img id=\"imgpmalogo\" src=\"./themes/pmahomme/img/logo_left.png\" alt=\"phpMyAdmin\">\n                </a>\n            </div>\n        </div>\n        <div id=\"pma_navigation_tree\" class=\"list_container synced highlight autoexpand\">\n            <div class=\"pma_quick_warp\">\n                <div class=\"drop_list\">\n                    <span title=\"Recent tables\" class=\"drop_button\">Recent</span>\n                </div>\n                <div class=\"drop_list\">\n                    <span title=\"Favorite tables\" class=\"drop_button\">Favorites</span>\n                </div>\n            </div>\n            <div class=\"clearfloat\"></div>\n            <ul>\n                <!-- Database list would go here -->\n            </ul>\n        </div>\n    </div>\n    <div id=\"pma_content_container\">\n        <div id=\"pma_content\">\n            <div id=\"maincontainer\">\n                <div id=\"main_pane_left\">\n                    <div class=\"group\">\n                        <h2>General settings</h2>\n                        <ul>\n                            <li><a class=\"item\" href=\"index.php?route=/server/databases\">Databases</a></li>\n                            <li><a class=\"item\" href=\"index.php?route=/server/sql\">SQL</a></li>\n                            <li><a class=\"item\" href=\"index.php?route=/server/status\">Status</a></li>\n                            <li><a class=\"item\" href=\"index.php?route=/server/privileges\">User accounts</a></li>\n                            <li><a class=\"item\" href=\"index.php?route=/server/export\">Export</a></li>\n                            <li><a class=\"item\" href=\"index.php?route=/server/import\">Import</a></li>\n                        </ul>\n                    </div>\n                </div>\n                <div id=\"main_pane_right\">\n                    <div class=\"group\">\n                        <h2>Database server</h2>\n                        <ul>\n                            <li>Server: 127.0.0.1</li>\n                            <li>Server type: MySQL</li>\n                            <li>Server version: 5.7.32 - MySQL Community Server (GPL)</li>\n                            <li>Protocol version: 10</li>\n                            <li>User: root@localhost</li>\n                            <li>Server charset: UTF-8 Unicode (utf8)</li>\n                        </ul>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>"}
{"path": "index.php", "args": {"route": "/server/sql"}, "content": "<div id=\"page_content\">\n    <form method=\"post\" action=\"index.php?route=/server/sql\" name=\"sqlform\" id=\"sqlform\" class=\"ajax lock-page\">\n        <div class=\"card mb-3\">\n            <div class=\"card-header\">Run SQL query/queries on server</div>\n            <div class=\"card-body\">\n                <textarea name=\"sql_query\" id=\"sqlquery\" cols=\"40\" rows=\"15\" class=\"form-control\"></textarea>\n            </div>\n            <div class=\"card-footer\">\n                <input type=\"hidden\" name=\"server\" value=\"1\">\n                <input type=\"hidden\" name=\"db\" value=\"\">\n                <input type=\"hidden\" name=\"table\" value=\"\">\n                <input type=\"hidden\" name=\"token\" value=\"token_value\">\n                <input class=\"btn btn-primary\" type=\"submit\" id=\"button_submit_query\" name=\"SQL\" value=\"Go\">\n            </div>\n        </div>\n    </form>\n    <div class=\"result_query\">\n        <table class=\"table table-striped table-hover table_results\" style=\"display: none;\">\n            <thead>\n                <tr>\n                    <th>Column 1</th>\n                    <th>Column 2</th>\n                    <th>Column 3</th>\n                </tr>\n            </thead>\n            <tbody>\n                <tr>\n                    <td>Data 1</td>\n                    <td>Data 2</td>\n                    <td>Data 3</td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</div>\n\n<div class=\"navigationbar\">\n    <a href=\"index.php?route=/server/databases\" class=\"btn\">Databases</a>\n    <a href=\"index.php?route=/server/sql\" class=\"btn\">SQL</a>\n    <a href=\"index.php?route=/server/status\" class=\"btn\">Status</a>\n    <a href=\"index.php?route=/server/export\" class=\"btn\">Export</a>\n    <a href=\"index.php?route=/server/import\" class=\"btn\">Import</a>\n</div>"}
